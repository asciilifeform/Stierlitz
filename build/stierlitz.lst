  36 0510                dbg_putchar:
  37 0510 0097               push   r2
  38 0512 0017               push   r0
  39 0514 67c0 00ff          and    r0, 0xFF
  40 0518 0002               mov    r2, r0
  41 051a 07c0 0001          mov    r0, 1      ; write to the UART
  42 051e af42               int    UART_INT   ; call UART_INT
  43 0520 05c0               pop    r0
  44 0522 05c2               pop    r2
  45 0524 cf97               ret
