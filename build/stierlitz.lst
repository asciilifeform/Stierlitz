  71 0996                do_rx_state_CBW:
  72 0996 07e7 16ea 094e     mov    w[usbrecv_addr], receive_buffer
  73 099c 07e7 001f 0950     mov    w[usbrecv_len], CBW_Size	; how many bytes to receive
